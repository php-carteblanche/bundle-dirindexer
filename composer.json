{
    "name": "carte-blanche/bundle-dirindexer",
    "type": "carte-blanche-bundle",
    "description": "A filesystem contents indexer",
    "keywords": [ "carte blanche", "bundles" ],
    "homepage": "http://github.com/php-carteblanche/bundle-dirindexer",
    "license": "Apache-2.0",
    "authors": [{
        "name": "Les Ateliers Pierrot & contributors",
        "email": "contact@ateliers-pierrot.fr",
        "homepage": "http://www.ateliers-pierrot.fr/"
    }],
    "require": {
        "atelierspierrot/patterns": "dev-master",
        "atelierspierrot/library": "dev-master",
        "atelierspierrot/webfilesystem": "dev-master",
        "carte-blanche/core": "dev-cb_wip"
    },
    "autoload": { 
        "psr-0": { "DirIndexer": "src" },
        "classmap": [ "src/DirIndexer/Controller" ]
    },
    "repositories": [{
        "type": "composer", "url": "http://packagist.ateliers-pierrot.fr/"
    }],
    "extra": {
        "views": "src/DirIndexer/views",
        "carte-blanche-configs": [ "config/dirindexer_config.ini" ],
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
